{
  "routes": [
    {
      "route": "api/experience/*",
      "rewrite": "https://kamksiteserver.azurewebsites.net/api/experience/*"
    },
    {
      "route": "api/about/*",
      "rewrite": "https://kamksiteserver.azurewebsites.net/api/about/*"
    },
    {
      "route": "/*",
      "serve": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
    "Access-Control-Allow-Headers": "*"
  }
}
